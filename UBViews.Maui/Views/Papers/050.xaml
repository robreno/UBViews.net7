<?xml version="1.0" encoding="utf-8"?><ContentPage xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit" xmlns:viewmodel="clr-namespace:UBViews.ViewModels" xmlns:model="clr-namespace:UBViews.Models" xmlns:local="UBViews" xmlns:control="clr-namespace:UBViews.Controls" xmlns:behavior="clr-namespace:UBViews.Behaviors" xmlns:attached="clr-namespace:UBViews.AttachedProperties" xmlns:converter="clr-namespace:UBViews.Converters" x:Class="UBViews.Views._050" x:DataType="viewmodel:XamlPaperViewModel" Title="The Evolution of Local Universes" xmlns="http://schemas.microsoft.com/dotnet/2021/maui"><ContentPage.Resources><ResourceDictionary><x:String x:Key="partTitle">The Evolution of Local Universes</x:String><x:String x:Key="paperTitle">The Planetary Princes</x:String><x:Int32 x:Key="paperId">50</x:Int32><x:String x:Key="author">Secondary Lanonandek</x:String><x:String x:Key="audioUri">https://s3.amazonaws.com/urantia/media/en/U50.mp3</x:String></ResourceDictionary><toolkit:TimeSpanToSecondsConverter x:Key="TimeSpanConverter" /><converter:SecondsToStringConverter x:Key="SecondsToStringConverter" /><converter:MediaStateChangedEventArgsConverter x:Key="MediaStateChangedEventArgsConverter" /><Style x:Key="basePidStyle" TargetType="Label"><Setter Property="Margin" Value="0,20,5,0" /><Setter Property="VerticalTextAlignment" Value="Center" /><Setter Property="IsVisible" Value="true" /></Style></ContentPage.Resources><ContentPage.Behaviors><behavior:EventToCommandBehavior EventName="Loaded" Command="{Binding PaperViewLoadedCommand}" CommandParameter="{Binding PaperDto}" /><behavior:EventToCommandBehavior EventName="Appearing" Command="{Binding PaperViewAppearingCommand}" CommandParameter="{Binding PaperDto}" /><behavior:EventToCommandBehavior EventName="Disappearing" Command="{Binding PaperViewDisappearingCommand}" CommandParameter="{Binding PaperDto}" /></ContentPage.Behaviors><Grid x:Name="contentGrid" Background