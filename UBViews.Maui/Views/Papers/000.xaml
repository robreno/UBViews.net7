        <behavior:EventToCommandBehavior EventName="Appearing" Command="{Binding PaperViewAppearingCommand}" CommandParameter="{Binding PaperDto}" />
        <behavior:EventToCommandBehavior EventName="Disappearing" Command="{Binding PaperViewDisappearingCommand}" CommandParameter="{Binding PaperDto}" />
    </ContentPage.Behaviors>
    <Grid x:Name="contentGrid" BackgroundColor="{AppThemeBinding Light={StaticResource LightBackground}, Dark={StaticResource UBDarkBackground}}">
