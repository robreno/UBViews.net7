                        <Label.FormattedText>
                            <FormattedString>
                                <Span Style="{StaticResource TabsSpan}" />
                                <Span x:Name="SP_000_065" Style="{StaticResource PID}" StyleId="0:3.11" Text="" />
                                <Span Style="{StaticResource RegularSpaceSpan}" />
                                <Span Style="{StaticResource RegularSpan}" Text="The First Source and Center is, therefore, primal in all domains: deified or undeified, personal or impersonal, actual or potential, finite or infinite. No thing or being, no relativity or finality, exists except in direct or indirect relation to, and dependence on, the primacy of the First Source and Center." />
                            </FormattedString>
