                    <Label x:Name="_000_016" Style="{StaticResource RegularParagraph}" ToolTipProperties.Text="0:1.8" attached:Audio.TimeSpan="0:4:60.0_0:4:68.0" attached:Ubml.UniqueId="016.000.001.008" attached:Ubml.SequenceId="0.16">
