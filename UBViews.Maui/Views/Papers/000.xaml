                                <Span x:Name="SP_000_011" Style="{StaticResource PID}" StyleId="0:1.3" Text="" />
                                <Span Style="{StaticResource RegularSpaceSpan}" />
                                <Span Style="{StaticResource RegularSpan}" Text="Deity functions on personal, prepersonal, and superpersonal levels. Total Deity is functional on the following seven levels:" />
                            </FormattedString>
                        </Label.FormattedText>
                    </Label>
                    <Label x:Name="_000_012" Style="{StaticResource RegularParagraph}" ToolTipProperties.Text="0:1.4" attached:Audio.TimeSpan="0:4:32.0_0:4:38.0" attached:Ubml.UniqueId="012.000.001.004" attached:Ubml.SequenceId="0.12">
                        <Label.GestureRecognizers>
