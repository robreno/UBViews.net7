                    <Label x:Name="_000_064" Style="{StaticResource RegularParagraph}" ToolTipProperties.Text="0:3.10" attached:Audio.TimeSpan="0:16:21.0_0:16:49.0" attached:Ubml.UniqueId="064.000.003.010" attached:Ubml.SequenceId="0.64">
