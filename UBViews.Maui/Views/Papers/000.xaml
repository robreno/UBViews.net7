                            <TapGestureRecognizer NumberOfTapsRequired="2" Command="{Binding Source={RelativeSource AncestorType={x:Type viewmodel:XamlPaperViewModel}}, Path=DoubleTappedGestureCommand}" CommandParameter="_000_012" />
                        </Label.GestureRecognizers>
                        <Label.FormattedText>
                            <FormattedString>
                                <Span Style="{StaticResource TabsSpan}" />
                                <Span x:Name="SP_000_012" Style="{StaticResource PID}" StyleId="0:1.4" Text="" />
                                <Span Style="{StaticResource RegularSpaceSpan}" />
                                <Span Style="{StaticResource ListNumberSpan}" Text="1." />
                                <Span Style="{StaticResource RegularSpan}" Text="  " />
                                <Span Style="{StaticResource ItalicSpan}" Text="Static" />
                                <Span Style="{StaticResource RegularSpan}" Text="—self-contained and self-existent Deity." />
                            </FormattedString>
                        </Label.FormattedText>
                    </Label>
                    <Label x:Name="_000_013" Style="{StaticResource RegularParagraph}" ToolTipProperties.Text="0:1.5" attached:Audio.TimeSpan="0:4:38.0_0:4:45.0" attached:Ubml.UniqueId="013.000.001.005" attached:Ubml.SequenceId="0.13">
