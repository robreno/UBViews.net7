<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit" xmlns:viewmodel="clr-namespace:UBViews.ViewModels" xmlns:model="clr-namespace:UBViews.Models" xmlns:local="UBViews" xmlns:control="clr-namespace:UBViews.Controls" xmlns:behavior="clr-namespace:UBViews.Behaviors" xmlns:attached="clr-namespace:UBViews.AttachedProperties" xmlns:converter="clr-namespace:UBViews.Converters" x:Class="UBViews.Views._000" x:DataType="viewmodel:XamlPaperViewModel" Title="Foreward" xmlns="http://schemas.microsoft.com/dotnet/2021/maui">
    <ContentPage.Resources>
        <ResourceDictionary>
            <x:String x:Key="partTitle">Foreward</x:String>
            <x:String x:Key="paperTitle">Foreword</x:String>
            <x:Int32 x:Key="paperId">0</x:Int32>
            <x:String x:Key="author">Divine Counselor</x:String>
