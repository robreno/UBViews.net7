                                <Span Style="{StaticResource RegularSpaceSpan}" />
                                <Span Style="{StaticResource ListNumberSpan}" Text="2." />
                                <Span Style="{StaticResource RegularSpan}" Text="  " />
                                <Span Style="{StaticResource ItalicSpan}" Text="Potential" />
                                <Span Style="{StaticResource RegularSpan}" Text="—self-willed and self-purposive Deity." />
                            </FormattedString>
                        </Label.FormattedText>
                    </Label>
                    <Label x:Name="_000_014" Style="{StaticResource RegularParagraph}" ToolTipProperties.Text="0:1.6" attached:Audio.TimeSpan="0:4:45.0_0:4:53.0" attached:Ubml.UniqueId="014.000.001.006" attached:Ubml.SequenceId="0.14">
                        <Label.GestureRecognizers>
                            <TapGestureRecognizer NumberOfTapsRequired="1" Command="{Binding Source={RelativeSource AncestorType={x:Type viewmodel:XamlPaperViewModel}}, Path=TappedGestureCommand}" CommandParameter="_000_014" />
