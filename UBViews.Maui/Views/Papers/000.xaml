                    <Label x:Name="_000_140" Style="{StaticResource RegularParagraph}" ToolTipProperties.Text="0:8.8" attached:Audio.TimeSpan="0:43:41.0_0:43:43.0" attached:Ubml.UniqueId="140.000.008.008" attached:Ubml.SequenceId="0.140">
