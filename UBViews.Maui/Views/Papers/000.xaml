                    <Label x:Name="_000_011" Style="{StaticResource RegularParagraph}" ToolTipProperties.Text="0:1.3" attached:Audio.TimeSpan="0:4:21.0_0:4:32.0" attached:Ubml.UniqueId="011.000.001.003" attached:Ubml.SequenceId="0.11">
