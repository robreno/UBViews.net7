                            <TapGestureRecognizer NumberOfTapsRequired="1" Command="{Binding Source={RelativeSource AncestorType={x:Type viewmodel:XamlPaperViewModel}}, Path=TappedGestureCommand}" CommandParameter="_000_075" />
