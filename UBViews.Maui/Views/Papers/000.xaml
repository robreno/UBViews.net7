                    <Label x:Name="_000_074" Style="{StaticResource RegularParagraph}" ToolTipProperties.Text="0:3.20" attached:Audio.TimeSpan="0:18:48.0_0:19:35.0" attached:Ubml.UniqueId="074.000.003.020" attached:Ubml.SequenceId="0.74">
