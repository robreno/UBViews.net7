                    <Label x:Name="_000_015" Style="{StaticResource RegularParagraph}" ToolTipProperties.Text="0:1.7" attached:Audio.TimeSpan="0:4:53.0_0:4:60.0" attached:Ubml.UniqueId="015.000.001.007" attached:Ubml.SequenceId="0.15">
