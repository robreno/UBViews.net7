                                <Span Style="{StaticResource RegularSpaceSpan}" />
                                <Span Style="{StaticResource TitleSpan}" Text="Foreword" />
                            </FormattedString>
                        </Label.FormattedText>
                    </Label>
                    <Label x:Name="_000_002" Style="{StaticResource OpeningParagraph}" ToolTipProperties.Text="0:0.1" attached:Audio.TimeSpan="0:0:2.0_0:0:48.0" attached:Ubml.UniqueId="002.000.000.001" attached:Ubml.SequenceId="0.2">
                        <Label.GestureRecognizers>
                            <TapGestureRecognizer NumberOfTapsRequired="1" Command="{Binding Source={RelativeSource AncestorType={x:Type viewmodel:XamlPaperViewModel}}, Path=TappedGestureCommand}" CommandParameter="_000_002" />
