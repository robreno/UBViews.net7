                        </Label.GestureRecognizers>
                        <Label.FormattedText>
                            <FormattedString>
                                <Span x:Name="SP_000_008" Style="{StaticResource PID}" StyleId="0:1.0" Text="" />
                                <Span Style="{StaticResource RegularSpaceSpan}" />
                                <Span Style="{StaticResource SectionSpan}" Text="I. Deity and Divinity" />
                            </FormattedString>
                        </Label.FormattedText>
                    </Label>
                    <Label x:Name="_000_009" Style="{StaticResource RegularParagraph}" ToolTipProperties.Text="0:1.1" attached:Audio.TimeSpan="0:3:37.0_0:3:52.0" attached:Ubml.UniqueId="009.000.001.001" attached:Ubml.SequenceId="0.9">
                        <Label.GestureRecognizers>
                            <TapGestureRecognizer NumberOfTapsRequired="1" Command="{Binding Source={RelativeSource AncestorType={x:Type viewmodel:XamlPaperViewModel}}, Path=TappedGestureCommand}" CommandParameter="_000_009" />
