                        </Label.FormattedText>
                    </Label>
                    <Label x:Name="_000_051" Style="{StaticResource RegularParagraph}" ToolTipProperties.Text="0:2.16" attached:Audio.TimeSpan="0:13:29.0_0:14:10.0" attached:Ubml.UniqueId="051.000.002.016" attached:Ubml.SequenceId="0.51">
