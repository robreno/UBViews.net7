                        </Label.GestureRecognizers>
                        <Label.FormattedText>
                            <FormattedString>
                                <Span Style="{StaticResource TabsSpan}" />
                                <Span x:Name="SP_000_007" Style="{StaticResource PID}" StyleId="0:0.6" Text="" />
                                <Span Style="{StaticResource RegularSpaceSpan}" />
                                <Span Style="{StaticResource RegularSpan}" Text="The seven evolving superuniverses in association with the central and divine universe, we commonly refer to as the " />
                                <Span Style="{StaticResource ItalicSpan}" Text="grand universe" />
                                <Span Style="{StaticResource RegularSpan}" Text="; these are the now organized and inhabited creations. They are all a part of the " />
                                <Span Style="{StaticResource ItalicSpan}" Text="master universe" />
                                <Span Style="{StaticResource RegularSpan}" Text=", which also embraces the uninhabited but mobilizing universes of outer space." />
                            </FormattedString>
                        </Label.FormattedText>
                    </Label>
                    <Label x:Name="_000_008" Style="{StaticResource SectionParagraph}" ToolTipProperties.Text="0:1.0" attached:Audio.TimeSpan="0:3:32.0_0:3:37.0" attached:Ubml.UniqueId="008.000.001.000" attached:Ubml.SequenceId="0.8">
                        <Label.GestureRecognizers>
