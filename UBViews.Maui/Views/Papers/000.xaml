                    </Label.FormattedText>
                </Label>
                <VerticalStackLayout x:Name="contentVerticalStackLayout" BackgroundColor="{AppThemeBinding Light={StaticResource LightBackground}, Dark={StaticResource DarkBackground}}" Style="{StaticResource PaperContent}">
                    <Label x:Name="_000_001" Style="{StaticResource TitleParagraph}" ToolTipProperties.Text="0:0.0" attached:Audio.TimeSpan="0:0:0.0_0:0:2.0" attached:Ubml.UniqueId="001.000.000.000" attached:Ubml.SequenceId="0.1">
