FormattedString></Label.FormattedText><FlyoutBase.ContextFlyout><MenuFlyout><MenuFlyoutItem Text="Copy" Command="{Binding Source={RelativeSource AncestorType={x:Type viewmodel:XamlPaperViewModel}}, Path=FlyoutMenuCommand}" CommandParameter="Copy_122_071" /><MenuFlyoutItem Text="Share" Command="{Binding Source={RelativeSource AncestorType={x:Type viewmodel:XamlPaperViewModel}}, Path=FlyoutMenuCommand}" CommandParameter="Share_122_071" /><MenuFlyoutItem Text="Email" Command="{Binding Source={RelativeSource AncestorType={x:Type viewmodel:XamlPaperViewModel}}, Path=FlyoutMenuCommand}" CommandParameter="Email_122_071" /></MenuFlyout></FlyoutBase.ContextFlyout></Label><Label x:Name="_122_072" Style="{StaticResource RegularParagraph}" ToolTipProperties.Text="122:9.11" attached:Audio.TimeSpan="0:32:15.155_0:32:19.207" attached:Ubml.UniqueId="072.122.009.011" attached:Ubml.SequenceId="122.72"><Label.GestureRecognizers><TapGestureRecognizer NumberOfTapsRequired="1" Command="{Binding Source={RelativeSource AncestorType={x:Type viewmodel:XamlPaperViewModel}}, Path=TappedGestureCommand}" CommandParameter="_122_072" /><TapGestureRecognizer NumberOfTapsRequired="2" Command="{Binding Source={RelativeSource AncestorType={x:Type viewmodel:XamlPaperViewModel}}, Path=DoubleTappedGestureCommand}" CommandParameter="_122_072" /></Label.GestureRecognizers><Label.FormattedText><FormattedString><Span Style="{StaticResource TabsSpan}" /><Span x:Name="SP_122_072" Style="{StaticResource PID}" StyleId="122:9.11" Text="" /><Span Style="{StaticResource RegularSpaceSpan}" /><Span Style="{StaticResource RegularSpan}" Text="To show mercy to our fathers, and remember his holy covenant—" /></FormattedString></Label.FormattedText><FlyoutBase.ContextFlyout><MenuFlyout><MenuFlyoutItem Text="Copy" Command="{Binding Source={RelativeSource AncestorType={x:Type viewmodel:XamlPaperViewModel}}, Path=FlyoutMenuCommand}" CommandParameter="Copy_122_072" /><MenuFlyoutItem Text="Share" Command="{Binding Source={Rela